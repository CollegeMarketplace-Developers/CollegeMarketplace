option_settings:
  - option_name: RDS_HOSTNAME
    value: /opt/elasticbeanstalk/bin/get-config environment -k RDS_HOSTNAME
  - option_name: RDS_PORT
    value: /opt/elasticbeanstalk/bin/get-config environment -k RDS_PORT
  - option_name: RDS_DB_NAME
    value: /opt/elasticbeanstalk/bin/get-config environment -k RDS_DB_NAME
  - option_name: RDS_USERNAME
    value: /opt/elasticbeanstalk/bin/get-config environment -k RDS_USERNAME
  - option_name: RDS_PASSWORD
    value: /opt/elasticbeanstalk/bin/get-config environment -k RDS_PASSWORD